{{#if loginerrors}}
  <div class="ui negative message transition">
    <i class="close icon"></i>
    <div class="header">
      There were some errors with your login details
    </div>
    <ul class="list">
      {{#each loginerrors}}
        <li>{{message}}</li>
      {{/each}}
    </ul>
  </div>
{{/if}}