<div class="ui row segment">
  <h3>Delete User(s)</h3>
  <form class="ui form" action="/admin/deleteusers" method="POST">
    <select name="users" multiple="" class="ui fluid dropdown">
      {{#each users}}
        <option value="{{_id}}">{{firstName}} {{lastName}}</option>
      {{/each}}
    </select><br>
    <button class="ui blue submit button">Delete</button>
  </form>
</div>